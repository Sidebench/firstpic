                  rendered="{!appType == 'baseball' || appType == 'healthy'}"
                >
                  <h5>
                    <apex:outputText
                      rendered="{!appType == 'baseball'}"
                      value="Badges for Baseball "
                    />
                    <apex:outputText
                      rendered="{!appType == 'healthy'}"
                      value="Healthy Lifestyles "
                    />
                    Curriculum Package
                  </h5>
                  <h6>
                    Please indicate the number of youth you will be serving
                    through this award and the number of staff that will be
                    instructing. This will help CRSF determine the amount of
                    instructional manuals and activity scorebooks to provide
                    based on inventory.
                    <apex:outputText
                      value=" Additional Badges for Baseball materials are available for download under the materials section of the CRSF Portal, www.ripkenfoundation.org/resource-portal."
                      rendered="{!appType == 'baseball'}"
                    />
                    <apex:outputText
                      value=" Additional Healthy Lifestyles materials are available for download under the materials section of the CRSF Portal www.ripkenfoundation.org/resource-portal."
                      rendered="{!appType == 'healthy'}"
                    />
                  </h6>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!appType == 'great'}">
                  <h5>I’m GREAT Curriculum Package</h5>
                  <h6>
                    Indicate the number of youth you will serve through this
                    award and the number of staff that will serve as mentors.
                    This will help CRSF determine the amount of instructional
                    manuals and activity workbooks to provide based on
                    inventory. Additional I’m GREAT materials are available for
                    download under the Resource Portal section of the Foundation
                    website: www.ripkenfoundation.org/resource-portal.
                  </h6>
                </apex:outputPanel>
                <div class="row">
                  <div class="col-xs-6">
                    <div class="semibold">
                      <span class="text-danger">*</span>
                      <apex:outputText
                        value="No. of youth to participate in Healthy Lifestyles sessions: "
                        rendered="{!appType == 'healthy'}"
                      />
                      <apex:outputText
                        value="No. of youth to participate in Badges for Baseball sessions: "
                        rendered="{!appType == 'baseball'}"
                      />
                      <apex:outputText
                        value="No. of girls to participate in I’m GREAT sessions: "
                        rendered="{!appType == 'great'}"
                      />
                    </div>
                    <apex:inputField
                      value="{!app.Number_Participating_in_HCHC_Sessions__c}"
                      rendered="{!NOT(readOnly)}"
                    />
                    <apex:outputField
                      value="{!app.Number_Participating_in_HCHC_Sessions__c}"
                      rendered="{!readOnly}"
                    />
                  </div>
                  <div class="col-xs-6">
                    <div class="semibold">
                      <span class="text-danger">*</span>
                      <apex:outputText
                        value="No. of Staff instructors:"
                        rendered="{!appType != 'great'}"
                      />
                      <apex:outputText
                        value="No. of female mentors:"
                        rendered="{!appType == 'great'}"
                      />
                    </div>
                    <apex:inputField
                      value="{!app.Number_of_Staff_Instructors__c}"
                      rendered="{!NOT(readOnly)}"
                    />
                    <apex:outputField
                      value="{!app.Number_of_Staff_Instructors__c}"
                      rendered="{!readOnly}"
                    />
                  </div>
                </div>

                <div class="row">
                  <div class="col-xs-12">
                    <div class="semibold">
                      <span class="text-danger">*</span>
                      <apex:outputPanel rendered="{!appType == 'baseball'}">
                        Please describe your organization’s plan for
                        implementing the Badges for Baseball program. Include
                        details on how you will incorporate the lessons with
                        current academic and/or athletic programming, how law
                        enforcement and other mentors will be involved, and what
                        each session will look like.
                      </apex:outputPanel>
                      <apex:outputPanel rendered="{!appType == 'healthy'}">
                        Please describe your organization’s plan for
                        implementing the Healthy Lifestyles program. Include
                        details on how you will incorporate the lessons with
                        current academic and/or athletic programming, how law
                        enforcement and other mentors will be involved, and what
                        each session will look like.
                      </apex:outputPanel>
                      <apex:outputPanel rendered="{!appType == 'great'}">
                        Describe your organization’s plans for implementing the
                        I’m GREAT character development element of the program.
                        Include details on how you will incorporate the lessons
                        with current programming, how female mentors will be
                        involved, and what each session will look like.
                      </apex:outputPanel>
                    </div>
                    <apex:inputField
                      value="{!app.Healthy_Choices_Implementation_Plan__c}"
                      rendered="{!NOT(readOnly)}"
                    />
                    <apex:outputField
                      value="{!app.Healthy_Choices_Implementation_Plan__c}"
                      rendered="{!readOnly}"
                    />
                  </div>
                </div>

                <div class="row">
                  <div class="col-xs-12">
                    <div class="semibold">
                      <span class="text-danger">*</span>
                      <apex:outputPanel rendered="{!appType == 'baseball'}">
                        Please provide a timeline that outlines your plans for
                        running CRSF programming over the course of the entire
                        year, from recruiting participants and mentors through
                        completing the Badges for Baseball curriculum by the end
                        of the year. The timeline can be approximate, but do
                        include how frequently you will implement the CRSF
                        program and how many youth you anticipate serving over
                        the course of the program.
                      </apex:outputPanel>
                      <apex:outputPanel rendered="{!appType == 'healthy'}">
                        Please provide a timeline that outlines your plans for
                        running CRSF programming over the course of the entire
                        year, from recruiting participants and mentors through
                        completing the HCHC curriculum by the end of the year.
                        The timeline can be approximate, but do include how
                        frequently you will implement the CRSF program and how
                        many youth you anticipate serving over the course of the
                        program.
                      </apex:outputPanel>
                      <apex:outputPanel rendered="{!appType == 'great'}">
                        Provide a timeline that outlines your plans for running
                        the I’m GREAT program over the course of the entire
                        grant, from recruiting participants and mentors through
                        completing the I’m GREAT curriculum by the end of the
                        grant. The timeline can be approximate, but do include
                        how frequently you will implement the I’m GREAT program
                        and how many youth you anticipate serving over the
                        course of the program.
                      </apex:outputPanel>
                    </div>
                    <apex:inputField
                      value="{!app.Timeline_for_Plans__c}"
                      rendered="{!NOT(readOnly)}"
                    />
                    <apex:outputField
                      value="{!app.Timeline_for_Plans__c}"
                      rendered="{!readOnly}"
                    />
                  </div>
                </div>

                <apex:outputPanel
                  rendered="{!appType == 'baseball' || appType == 'healthy'}"
                >
                  <div class="row">
                    <div class="col-xs-12">
                      <apex:outputPanel rendered="{!appType == 'baseball'}">
                        <div class="semibold">
                          <span class="text-danger">*</span>
                          Please describe your organization’s plans for
                          implementing team sports
                          (baseball/softball/Quickball/other activities)
                          elements of the program. Include the following: What
                          sports or activities will you use? Competitive league
                          or recreations? Do you have access to a field?
                        </div>
                      </apex:outputPanel>
                      <apex:outputPanel rendered="{!appType == 'healthy'}">
                        <div class="semibold">
                          <span class="text-danger">*</span>
                          What sports, if any, do you plan to incorporate into
                          your Healthy Lifestyles program? This could include
                          training for specific sports with specific sports
                          teams or just incorporating sport activities into your
                          general fitness program.
                        </div>
                      </apex:outputPanel>
                      <apex:inputField
                        value="{!app.Team_Sports_Implementation_Plan__c}"
                        rendered="{!NOT(readOnly)}"
                      />
                      <apex:outputField
                        value="{!app.Team_Sports_Implementation_Plan__c}"
                        rendered="{!readOnly}"
                      />
                    </div>
                  </div>
                </apex:outputPanel>

                <div class="row">
                  <div class="col-xs-12">
                    <div class="semibold">
                      <span class="text-danger">*</span>
                      <apex:outputText
                        value="Please list school opening and closing dates within your current area."
                        rendered="{!appType == 'baseball' || appType == 'healthy'}"
                      />
                      <apex:outputText
                        value="List school opening and closing dates for youth who attend your organization."
                        rendered="{!appType == 'great'}"
                      />
                    </div>
                    <apex:inputField
                      value="{!app.School_Opening_and_Closing_Dates__c}"
                      rendered="{!NOT(readOnly)}"
                    />
                    <apex:outputField
                      value="{!app.School_Opening_and_Closing_Dates__c}"
                      rendered="{!readOnly}"
                    />
                  </div>
                </div>

                <hr />

                <div class="row">
                  <div class="col-xs-6">
                    <span class="semibold"
                      ><span class="text-danger">*</span>
                      Proposed Program Start Date: &nbsp;
                    </span>
                    <apex:inputField
                      value="{!app.Grant_Start_Date__c}"
                      styleClass="input-sm"
                      rendered="{!NOT(readOnly)}"
                    />
                    <apex:outputField
                      value="{!app.Grant_Start_Date__c}"
                      styleClass="input-sm"
                      rendered="{!readOnly}"
                    />
                  </div>
                  <div class="col-xs-6">
                    <span class="semibold"
                      ><span class="text-danger">*</span>
                      Proposed Program End Date: &nbsp;
                    </span>
                    <apex:inputField
                      value="{!app.Grant_End_Date__c}"
                      styleClass="input-sm"
                      rendered="{!NOT(readOnly)}"
                    />
                    <apex:outputField
                      value="{!app.Grant_End_Date__c}"
                      styleClass="input-sm"
                      rendered="{!readOnly}"
                    />
                  </div>
                </div>

                <div class="row">
                  <div class="col-xs-12">
                    <div class="semibold">
                      <span class="text-danger">*</span>
                      <apex:outputPanel
                        rendered="{!appType == 'baseball' || appType == 'healthy'}"
                      >
                        Please describe how your organization’s capacity to
                        recruit and engage community partners, including law
                        enforcement professionals and volunteers, has affected
                        the program. Please detail any current relationships
                        your organization has with local law enforcement
                        agencies, community service groups,
                        universities/colleges, local business, etc.
                      </apex:outputPanel>
                      <apex:outputPanel rendered="{!appType == 'great'}">
                        Describe your organization’s capacity to recruit and
                        engage community partners. Detail any current
                        relationships that your organization has with local
                        community service groups, universities/colleges,
                        businesses, etc.
                      </apex:outputPanel>
                    </div>
                    <apex:inputField
                      value="{!app.Plan_to_Connect_Youth_with_Adult_Mentors__c}"
                      rendered="{!NOT(readOnly)}"
                    />
                    <apex:outputField
                      value="{!app.Plan_to_Connect_Youth_with_Adult_Mentors__c}"
                      rendered="{!readOnly}"
                    />
                  </div>
                </div>

                <div class="row">
                  <div class="col-xs-12">
                    <div class="semibold">
                      <span class="text-danger">*</span>
                      Please describe your organization’s knowledge of, or
                      relationship with, local military and/or youth with a
                      parent in the military.
                    </div>
                    <apex:inputField
                      value="{!app.Orgs_Knowledge_of_Military__c}"
                      rendered="{!NOT(readOnly)}"
                    />
                    <apex:outputField
                      value="{!app.Orgs_Knowledge_of_Military__c}"
                      rendered="{!readOnly}"
                    />
                  </div>
                </div>

                <div class="row">
                  <div class="col-xs-12">
                    <div class="semibold">
                      <span class="text-danger">*</span>
                      Please include any additional information you would like
                      to know about the service CRSF will provide along with
                      this award, or questions you may have about the
                      implementation of the program.
                    </div>
                    <apex:inputField
                      value="{!app.Additional_Information__c}"
                      rendered="{!NOT(readOnly)}"
                    />
                    <apex:outputField
                      value="{!app.Additional_Information__c}"
                      rendered="{!readOnly}"
                    />
                  </div>
                </div>
                <apex:outputPanel
                  rendered="{!LEFT(TEXT(app.CreatedDate),4) == '2020' || LEFT(TEXT(app.CreatedDate),4) == '2021'}"
                >
                  <div class="row">
                    <div class="col-xs-12">
                      <div class="semibold">
                        <span class="text-danger">*</span>
                        Please list applicable social media tags/usernames for
                        your organization (Facebook, Twitter, Instagram).
                      </div>
                      <apex:inputField
                        value="{!app.Social_Media_Usernames__c}"
                        rendered="{!NOT(readOnly)}"
                      />
                      <apex:outputField
                        value="{!app.Social_Media_Usernames__c}"
                        rendered="{!readOnly}"
                      />
                    </div>
                  </div>
                </apex:outputPanel>
              </div>
            </div>
          </apex:outputPanel>

          <c:Attachments
            parentId="{!app.Id}"
            fileName="Tax Status,Other"
            showUploadBtn="{!userAcctType != 'National Organization' && NOT(readOnly)}"
            requiredText="(Tax Document Required)"
          />

          <apex:outputPanel
            rendered="{!userAcctType == 'Local Organization' && NOT(readOnly)}"
          >
            <div class="alert alert-warning" role="alert">
              Please attach your organizations 501c3 tax document. If not
              applicable, please reach out to your Cal Ripken, Sr. Foundation
              Program Coordinator for clarification.
            </div>
          </apex:outputPanel>

          <apex:outputPanel id="notesPopup">
            <c:NotesPopup
              header="Send Back"
              displayNotesPopup="{!displayNotesPopup}"
              userAcctType="{!userAcctType}"
              displayLocalNote="false"
              nationalNote="{!app.Sent_Back_Reason__c}"
            >
              <apex:commandButton
                action="{!hideNotesPopup}"
                value="Close"
                status="pageStatus"
                immediate="true"
                reRender="notesPopup"
              />
              <apex:commandButton
                value="Send Back"
                action="{!sendBack}"
                styleClass="btn primary"
              />
              <apex:pageMessages />
            </c:NotesPopup>
          </apex:outputPanel>

          <apex:commandButton
            value="Submit"
            action="{!submit}"
            styleClass="btn primary"
            rendered="{!userAcctType != 'National Organization' && NOT(readOnly)}"
          />
          <apex:outputPanel
            rendered="{!userAcctType == 'National Organization' && 
                                                (app.Status__c == 'Submitted' || app.Status__c == 'Awarded') && 
                                                (app.Organization__c == null || app.Organization__r.Primary_Approver__c == currentUser.ContactId  || app.Organization__r.Secondary_Approver__c == currentUser.ContactId|| approverLevel == 'Finance Admin')}"
          >
            <apex:commandButton
              value="Send Back"
              action="{!showSendBackPopup}"
              styleClass="btn"
              rendered="{!app.Status__c != 'Awarded'}"
              reRender="notesPopup"
            />
            <apex:commandButton
              value="Award"
              action="{!URLFOR($Site.Prefix+'/AwardApplication?appId='+app.Id)}"
              styleClass="btn primary"
            />
          </apex:outputPanel>
          <c:ErrorMessage errorMessage="{!errorMessage}" />
          <apex:pageMessages />
        </apex:outputPanel>
        <br />
      </div>
    </div>
  </apex:form>
</apex:page>
