<apex:page
  controller="ApplicationCtrl"
  showHeader="false"
  sidebar="false"
  docType="html-5.0"
>
  <link
    href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css"
    rel="stylesheet"
    media="screen"
  />
  <c:RipkenHeader
    ActivePage="applications"
    acct="{!userAcct}"
    profile="{!currentUser.Profile.Name}"
    rendered="{!userAcctType == 'National Organization'}"
  />
  <head>
    <title>Grant Application</title>
    <style>
      h2,
      .h2 {
        font-size: 24px;
        margin: 6px 0 18px;
        text-transform: uppercase;
        font-family: trebuchet ms;
        color: #555;
      }
      h5,
      .h5 {
        font-size: 16px;
        border-bottom: 1px solid #ccc;
      }
      h6,
      .h6 {
        font-size: 12px;
        color: #777;
        display: block;
        margin: 2px 0 4px;
        font-weight: normal;
      }
      input[type="text"],
      select,
      textarea {
        display: block;
        width: 100%;
      }
      .input-sm,
      .datePicker select {
        display: inline !important;
        width: auto !important;
      }
      .row {
        padding: 6px 0 10px;
      }
      .grid-pd [class*="col-"] {
        padding-bottom: 10px;
      }

      a,
      .primary {
        color: #f67c1c;
      }
      a:hover,
      a:focus {
        color: #ef6f09 !important;
      }
      input[type="submit"].btn.primary,
      .btn.primary {
        background: #f47f38;
        background-image: -webkit-linear-gradient(top, #f67c1c, #ef6f09);
        background-image: -moz-linear-gradient(top, #f67c1c, #ef6f09);
        background-image: -ms-linear-gradient(top, #f67c1c, #ef6f09);
        background-image: -o-linear-gradient(top, #f67c1c, #ef6f09);
        background-image: linear-gradient(to bottom, #f67c1c, #ef6f09);
        color: #fff;
        padding: 4px 12px;
      }
      input[type="submit"].primary:hover,
      .btn.primary:hover {
        background: #ef6f09 !important;
        text-decoration: none;
      }
      .col-auto {
        float: left;
        position: relative;
        min-height: 1px;
        padding-right: 15px;
        padding-left: 15px;
      }
      .custPopup {
        z-index: 999;
        position: fixed;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
      }
      .custPopup .modal-dialog,
      .custPopup .modal-content {
        height: 80%;
      }
      .custPopup .modal-body {
        max-height: 400px;
        overflow-y: scroll;
      }
      .popupBackground {
        background-color: black;
        opacity: 0.2;
        filter: alpha(opacity = 20);
        position: fixed;
        width: 100%;
        height: 100%;
        top: 0;
        left: 0;
        z-index: 99;
      }
    </style>
  </head>

  <apex:form id="form" style="padding: 0 20px">
    <div class="main-panel">
      <div class="content">
        <apex:outputPanel rendered="{!userAcctType == 'National Organization'}">
          <div class="row">
            <div class="col-auto">
              <h2>{!title} Grant Application</h2>
            </div>
            <div
              class="col-auto pull-right"
              style="font-size: 1.2em; padding-top: 10px"
            >
              Status: {!app.Status__c}
            </div>
          </div>
        </apex:outputPanel>
        <apex:outputPanel rendered="{!userAcctType != 'National Organization'}">
          <h3 style="display: block; text-align: center; margin: 15px 0">
            {!title} Grant Application
          </h3>
        </apex:outputPanel>
        <apex:outputPanel rendered="{!showSuccess}">
          <div
            class="alert alert-success"
            role="alert"
            style="margin-top: 20px"
          >
            You have successfully submitted your application.
          </div>
        </apex:outputPanel>
        <apex:outputPanel rendered="{!NOT(showSuccess)}">
          <c:ErrorMessage errorMessage="{!errorMessage}" />
          <div class="text-danger">* Required Fields</div>

          <div class="panel panel-default">
            <div class="panel-heading">
              <h1 class="panel-title">Organizational Information</h1>
            </div>
            <div class="panel-body">
              <div class="row">
                <div class="col-xs-8">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Legal Name:
                  </div>
                  <apex:inputField
                    value="{!app.Organization_Name__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Organization_Name__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    EIN Number:
                  </div>
                  <apex:inputField
                    value="{!app.EIN__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.EIN__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>

              <div class="row">
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Website URL:
                  </div>
                  <apex:inputField
                    value="{!app.Website__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Website__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Telephone:
                  </div>
                  <apex:inputField
                    value="{!app.Phone__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Phone__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">Fax:</div>
                  <apex:inputField
                    value="{!app.Fax__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Fax__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>

              <div class="row">
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Physical Address:
                  </div>
                  <apex:inputField
                    value="{!app.Address__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Address__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    City:
                  </div>
                  <apex:inputField
                    value="{!app.City__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.City__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-2">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    State:
                  </div>
                  <apex:inputField
                    value="{!app.State__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.City__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    ZipCode:
                  </div>
                  <apex:inputField
                    value="{!app.Postal_Zip_Code__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Postal_Zip_Code__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>

              <div class="row">
                <div class="col-xs-4">
                  <div class="semibold">Mailing Address:</div>
                  <apex:inputField
                    value="{!app.Mailing_Address__c}"
                    style="display: block"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Mailing_Address__c}"
                    style="display: block"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">City:</div>
                  <apex:inputField
                    value="{!app.Mailing_City__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Mailing_City__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-2">
                  <div class="semibold">State:</div>
                  <apex:inputField
                    value="{!app.Mailing_State__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Mailing_State__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">ZipCode:</div>
                  <apex:inputField
                    value="{!app.Mailing_Postal_Code__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Mailing_Postal_Code__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-12">
                  <apex:inputCheckbox
                    value="{!app.Mailing_Address_Same_as_Physical_Address__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Mailing_Address_Same_as_Physical_Address__c}"
                    rendered="{!readOnly}"
                  />
                  Same as Physical Address?
                </div>
              </div>
            </div>
          </div>

          <div class="panel panel-default">
            <div class="panel-heading">
              <h1 class="panel-title">Organization Background</h1>
            </div>
            <div class="panel-body">
              <div class="row">
                <div class="col-xs-6">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Annual Budget ($):
                  </div>
                  <apex:inputField
                    value="{!app.Total_Budget__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Total_Budget__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-6">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    No. of Sites Organization represents:
                  </div>
                  <apex:inputField
                    value="{!app.Number_of_Sites__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Number_of_Sites__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>

              <h5 style="border-bottom: 1px solid #ccc">
                <span class="text-danger">*</span>Community Setting
              </h5>
              <div class="row">
                <div class="col-xs-4">
                  <div class="semibold">% Urban:</div>
                  <apex:inputField
                    value="{!app.Urban__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Urban__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">% Suburban:</div>
                  <apex:inputField
                    value="{!app.Suburban__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Suburban__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">% Rural:</div>
                  <apex:inputField
                    value="{!app.Rural__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Rural__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>

              <div class="row">
                <div class="col-xs-6">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Organization Type:
                  </div>
                  <apex:inputField
                    value="{!app.Type__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Type__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-6">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Additional Org Classification:
                  </div>
                  <apex:inputField
                    value="{!app.Additional_Org_Classification__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Additional_Org_Classification__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>

              <div class="row">
                <div class="col-xs-12">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Mission Statement &amp; Organization Background:
                  </div>
                  <apex:inputField
                    value="{!app.Mission_Statement__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Mission_Statement__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>
            </div>
          </div>

          <div class="panel panel-default">
            <div class="panel-heading">
              <h1 class="panel-title">Organization Overall Demographics</h1>
            </div>
            <div class="panel-body">
              <div class="row">
                <div class="col-xs-12">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Number of Youth Served per Year:
                  </div>
                  <apex:inputField
                    value="{!app.Number_of_Youth_Served_per_Year__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Number_of_Youth_Served_per_Year__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>

              <div class="row">
                <div class="col-xs-3">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Min Age of Youth:
                  </div>
                  <apex:inputField
                    value="{!app.Min_Age_of_Youth__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Min_Age_of_Youth__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Max Age of Youth:
                  </div>
                  <apex:inputField
                    value="{!app.Max_Age_of_Youth__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Max_Age_of_Youth__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    % Male:
                  </div>
                  <apex:inputField
                    value="{!app.Percent_Male__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_Male__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    % Female:
                  </div>
                  <apex:inputField
                    value="{!app.Percent_Female__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_Female__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>

              <div class="row">
                <div class="col-xs-3">
                  <div class="semibold">% American Indian/Alaska Native:</div>
                  <apex:inputField
                    value="{!app.Percent_American_Indian_Alaska_Native__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_American_Indian_Alaska_Native__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">% Asian:</div>
                  <apex:inputField
                    value="{!app.Percent_Asian__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_Asian__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">% Black or African American:</div>
                  <apex:inputField
                    value="{!app.Percent_Black_or_African_American__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_Black_or_African_American__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">% Hispanic or Latino:</div>
                  <apex:inputField
                    value="{!app.Percent_Hispanic_or_Latino__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_Hispanic_or_Latino__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>

              <div class="row">
                <div class="col-xs-3">
                  <div class="semibold">
                    % Native Hawaiian/Other Pacific Islander:
                  </div>
                  <apex:inputField
                    value="{!app.Percent_Native_Hawaiian_Pacific_Islander__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_Native_Hawaiian_Pacific_Islander__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">% White:</div>
                  <apex:inputField
                    value="{!app.Percent_White__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_White__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">% More than One Race:</div>
                  <apex:inputField
                    value="{!app.Percent_More_than_One_Race__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_More_than_One_Race__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-3">
                  <div class="semibold">% Race Not Indicated:</div>
                  <apex:inputField
                    value="{!app.Percent_Race_Not_Indicated__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_Race_Not_Indicated__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>

              <div class="row">
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    % Youth eligible for free/reduced-price lunch:
                  </div>
                  <apex:inputField
                    value="{!app.Percent_Eligible_Free_Reduced_Lunch__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_Eligible_Free_Reduced_Lunch__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    % Youth in Single-Parent Homes:
                  </div>
                  <apex:inputField
                    value="{!app.Percent_Youth_in_Single_Parent_Homes__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Percent_Youth_in_Single_Parent_Homes__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    % Youth living at/below poverty level:
                  </div>
                  <apex:inputField
                    value="{!app.Youth_Living_at_or_below_Poverty_Level__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.Youth_Living_at_or_below_Poverty_Level__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>
            </div>
          </div>

          <div class="panel panel-default">
            <div class="panel-heading">
              <h1 class="panel-title">Application &amp; Program Contacts</h1>
            </div>
            <div class="panel-body">
              <h5>Chief Professional Officer, Executive Director, President</h5>
              <h6>Contact who oversees organizational operations.</h6>
              <div class="row">
                <div class="col-sm-1">
                  <div class="semibold">Prefix:</div>
                  <apex:inputField
                    value="{!app.CEO_Prefix__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.CEO_Prefix__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    First Name:
                  </div>
                  <apex:inputField
                    value="{!app.CEO_First_Name__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.CEO_First_Name__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Last Name:
                  </div>
                  <apex:inputField
                    value="{!app.CEO_Last_Name__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.CEO_Last_Name__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-sm-1">
                  <div class="semibold">Suffix:</div>
                  <apex:inputField
                    value="{!app.CEO_Suffix__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.CEO_Suffix__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>
              <div class="row">
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Position Title:
                  </div>
                  <apex:inputField
                    value="{!app.CEO_Title__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.CEO_Title__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Role in Program:
                  </div>
                  <apex:inputField
                    value="{!app.CEO_Role__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.CEO_Role__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Email:
                  </div>
                  <apex:inputField
                    value="{!app.CEO_Email__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.CEO_Email__c}"
                    rendered="{!readOnly}"
                  />
                </div>
              </div>
              <div class="row">
                <div class="col-xs-4">
                  <div class="semibold">
                    <span class="text-danger">*</span>
                    Day Phone Number:
                  </div>
                  <apex:inputField
                    value="{!app.CEO_Phone__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.CEO_Phone__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">Extension:</div>
                  <apex:inputField
                    value="{!app.CEO_Ext__c}"
                    rendered="{!NOT(readOnly)}"
                  />
                  <apex:outputField
                    value="{!app.CEO_Ext__c}"
                    rendered="{!readOnly}"
                  />
                </div>
                <div class="col-xs-4">
                  <div class="semibold">Cell Phone Number:</div>
                  <apex:inputField
