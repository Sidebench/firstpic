                  <td class="radioBtnTbl" style="width: 80px">
                    <apex:selectRadio value="{!assess.S9g_Waiting_Period__c}">
                      <apex:selectOptions value="{!options}" />
                    </apex:selectRadio>
                  </td>
                  <td>A waiting period before overnight visits can occur</td>
                  <td class="pull-right">
                    <apex:inputField
                      value="{!assess.S9g_Waiting_Period_Text__c}"
                      styleClass="input-sm"
                    />
                  </td>
                </tr>
                <tr>
                  <td class="radioBtnTbl" style="width: 80px">
                    <apex:selectRadio value="{!assess.S9g_Accommodations__c}">
                      <apex:selectOptions value="{!options}" />
                    </apex:selectRadio>
                  </td>
                  <td>
                    Requirements for accommodations that allow for privacy for
                    sleeping and changing clothes
                  </td>
                  <td class="pull-right">
                    <apex:inputField
                      value="{!assess.S9g_Accommodations_Text__c}"
                      styleClass="input-sm"
                    />
                  </td>
                </tr>
                <tr>
                  <td class="radioBtnTbl" style="width: 80px">
                    <apex:selectRadio
