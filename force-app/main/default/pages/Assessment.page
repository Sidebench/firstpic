                      <apex:selectOptions value="{!options}" />
                    </apex:selectRadio>
                  </td>
                  <td>Match Supervision and Support</td>
                  <td class="pull-right">
                    <apex:inputField
                      value="{!assess.S12_Match_Supervision_Text__c}"
                      styleClass="input-sm"
                    />
                  </td>
                </tr>
                <tr>
                  <td class="radioBtnTbl" style="width: 80px">
                    <apex:selectRadio value="{!assess.S12_Match_Closure__c}">
