                      <apex:selectOptions value="{!options}" />
                    </apex:selectRadio>
                  </td>
                  <td>
                    Ensuring there are qualified, diverse candidates in the
                    selection pool
                  </td>
                  <td class="pull-right">
                    <apex:inputField
                      value="{!assess.S7b_Qualified_Diverse_Candidates_Text__c}"
                      styleClass="input-sm"
                    />
                  </td>
                </tr>
                <tr>
                  <td class="radioBtnTbl" style="width: 80px">
                    <apex:selectRadio
                      value="{!assess.S7b_CEO_Orientation_Confirmation__c}"
