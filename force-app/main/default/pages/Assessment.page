                      <apex:selectOptions value="{!options}" />
                    </apex:selectRadio>
                  </td>
                  <td>Confidentiality</td>
                  <td class="pull-right">
                    <apex:inputField
                      value="{!assess.S9i_Confidentiality_Text__c}"
                      styleClass="input-sm"
                    />
                  </td>
                </tr>
                <tr>
                  <td class="radioBtnTbl" style="width: 80px">
                    <apex:selectRadio value="{!assess.S9i_Privacy__c}">
                      <apex:selectOptions value="{!options}" />
                    </apex:selectRadio>
                  </td>
                  <td>Privacy</td>
                  <td class="pull-right">
                    <apex:inputField
                      value="{!assess.S9i_Privacy_Text__c}"
                      styleClass="input-sm"
                    />
                  </td>
