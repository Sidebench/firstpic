                      />
                    </apex:selectList>
                    <!-- <apex:outputField value="{!report.Clients_Surveyed_for_Feedback__c}" rendered="{!readOnly}" />-->
                  </div>
                </div>
                <apex:outputPanel
                  rendered="{!NOT(onTrackFiscally) || NOT(report.On_Track_Fiscally__c)}"
                >
                  <div class="row">
                    <div class="col-xs-1"></div>
                    <div class="col-xs-11">
                      Please Explain:<font color="red"> *</font>
                    </div>
                    <div class="col-xs-1"></div>
                    <div class="col-xs-11">
                      <apex:inputField
                        value="{!report.Fiscally_on_Track__c}"
