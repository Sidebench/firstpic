                        escape="false"
                      /><font color="red"> *</font>
                    </div>
                    <div class="col-xs-12">
                      <apex:selectList
                        disabled="{!readOnly}"
                        value="{!clientsSurveyed}"
                        size="1"
                      >
                        <apex:selectOption itemLabel="Yes" itemValue="true" />
                        <apex:selectOption itemLabel="No" itemValue="false" />
                        <apex:actionSupport
                          event="onchange"
                          action="{!updateFieldVisibility}"
                          reRender="form"
                        />
                      </apex:selectList>
                      <!--<apex:outputField value="{!report.Clients_Surveyed_for_Feedback__c}" rendered="{!readOnly}" />-->
                    </div>
                  </div>
                  <apex:outputPanel
                    rendered="{!clientsSurveyed || report.Clients_Surveyed_for_Feedback__c}"
                  >
                    <div class="row">
                      <div class="col-xs-12">
                        <apex:outputText
                          value="{!labels['Number_of_Surveys_Distributed__c']}"
                          escape="false"
                        /><font color="red"> *</font>
                      </div>
                      <div class="col-xs-12">
                        <apex:inputField
                          value="{!report.Number_of_Surveys_Distributed__c}"
                          rendered="{!NOT(readOnly)}"
                        />
                        <apex:outputText
                          value="{!report.Number_of_Surveys_Distributed__c}"
                          rendered="{!readOnly}"
                        />
                      </div>
                    </div>

                    <div class="row">
                      <div class="col-xs-12">
                        <apex:outputText
                          value="{!labels['Number_of_Surveys_Completed__c']}"
                          escape="false"
                        /><font color="red"> *</font>
                      </div>
                      <div class="col-xs-12">
                        <apex:inputField
                          value="{!report.Number_of_Surveys_Completed__c}"
                          rendered="{!NOT(readOnly)}"
                        />
                        <apex:outputText
                          value="{!report.Number_of_Surveys_Completed__c}"
                          rendered="{!readOnly}"
                        />
                      </div>
                    </div>
                  </apex:outputPanel>

                  <div class="row">
                    <div class="col-xs-12">
                      <apex:outputText
                        value="{!labels['Challenges_During_Fiscal_Year__c']}"
                        escape="false"
                      /><font color="red"> *</font>
                    </div>
                    <div class="col-xs-12">
                      <apex:inputField
                        value="{!report.Challenges_During_Fiscal_Year__c}"
                        rendered="{!NOT(readOnly)}"
                      />
                      <apex:outputText
                        value="{!report.Challenges_During_Fiscal_Year__c}"
                        rendered="{!readOnly}"
                      />
                    </div>
                  </div>

                  <div class="row">
                    <div class="col-xs-12">
                      <apex:outputText
                        value="{!labels['Project_Description__c']}"
                        escape="false"
                      /><font color="red"> *</font>
                    </div>
                    <div class="col-xs-12">
                      <apex:inputField
                        value="{!report.Project_Description__c}"
                        rendered="{!NOT(readOnly)}"
                      />
                      <apex:outputText
                        value="{!report.Project_Description__c}"
                        rendered="{!readOnly}"
                      />
                    </div>
                  </div>

                  <div class="row">
                    <div class="col-xs-12">
                      <apex:outputText
                        value="{!labels['Changes_in_Community__c']}"
                        escape="false"
                      /><font color="red"> *</font>
                    </div>
                    <div class="col-xs-12">
                      <apex:inputField
                        value="{!report.Changes_in_Community__c}"
                        rendered="{!NOT(readOnly)}"
                      />
                      <apex:outputText
                        value="{!report.Changes_in_Community__c}"
                        rendered="{!readOnly}"
                      />
                    </div>
                  </div>
                </div>
              </div>
            </apex:outputPanel>
          </apex:outputPanel>

          <apex:outputPanel
            rendered="{!userAcctType == 'National Organization' || (userAcctType == 'Local Organization' && report.Notes_from_National__c != null)}"
          >
            <div class="panel panel-default">
              <div class="panel-body grid-pd-md">
                <strong>Notes From National:</strong><br />
                <apex:outputText
                  escape="false"
                  value="{!report.Notes_from_National__c}"
                />
              </div>
            </div>
          </apex:outputPanel>
          <apex:outputPanel rendered="{!userAcctType == 'Local Organization'}">
            <h5>
              Please contact
              <a href="mailto:support@firstpic.org">support@firstpic.org</a> if
              you have any questions about completing or submitting this report.
            </h5>
          </apex:outputPanel>
          <div class="row">
            <div class="col-md-6">
              <apex:commandButton
                action="{!previous}"
                reRender="form"
                value="Previous"
                status="pageStatus"
                rendered="{!currentPage > 1}"
                oncomplete="scrollTop()"
              />
              <apex:commandButton
                action="{!save}"
                reRender="form"
                value="Save"
                status="pageStatus"
                rendered="{!NOT(readOnly) || (userAcctType == 'National Organization' && report.Status__c == 'Submitted')}"
              />
              <apex:commandButton
                action="{!next}"
                reRender="form"
                value="Next"
                status="pageStatus"
                rendered="{!currentPage < 4}"
                styleClass="primary"
                oncomplete="scrollTop()"
              />
              <apex:commandButton
                action="{!submit}"
                reRender="form"
                value="Submit"
                status="pageStatus"
                styleClass="primary"
                rendered="{!currentPage == 4 && userAcctType == 'Local Organization' && NOT(readOnly)}"
              />
              <apex:commandButton
                action="{!showSendBackPopup}"
                value="Send Back"
                status="pageStatus"
                rendered="{!currentPage == 4 && userAcctType == 'National Organization' && report.Status__c == 'Submitted'}"
              />
              <apex:commandButton
                action="{!approve}"
                value="Approve"
                status="pageStatus"
                styleClass="primary"
                rendered="{!currentPage == 4 && userAcctType == 'National Organization' && report.Status__c == 'Submitted'}"
