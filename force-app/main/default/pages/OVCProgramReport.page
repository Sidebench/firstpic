          </div>
        </apex:outputPanel>

        <apex:outputPanel id="validationPopup">
          <c:ConfirmationPopup
            displayConfirmPopup="{!displayValidationPopup}"
            header="Types of Victimizations Not Tracked"
            body="{!notTrackedMessage}"
          >
            <apex:commandButton
              action="{!hideValidationPopup}"
              value="Cancel"
              reRender="validationPopup"
            />
            <apex:commandButton
              action="{!hideValidationPopupAndContinue}"
              value="Confirm"
              styleClass="primary"
              reRender="form"
            />
          </c:ConfirmationPopup>
        </apex:outputPanel>

        <apex:outputPanel id="sendBackPopup">
          <c:NotesPopup
            userAcctType="{!userAcctType}"
            header="Send Back"
            richText="true"
            displayNotesPopup="{!displaySendBackPopup}"
            nationalNote="{!report.Notes_from_National__c}"
            readOnly="false"
            displayNationalNote="true"
            displayLocalNote="false"
          >
            <apex:commandButton
              action="{!cancelSendBackPopup}"
              value="Cancel"
            />
            <apex:commandButton
              action="{!sendBack}"
              value="Send Back"
              styleClass="primary"
            />
          </c:NotesPopup>
        </apex:outputPanel>

        <br />
        <apex:pageMessages></apex:pageMessages>
        <c:ErrorMessage errorMessage="{!pageTwoErrorMessage}" />
        <c:ErrorMessage errorMessage="{!errorMessage}" />
        <!--                <c:ErrorMessage errorMessage="{!successMessage}" type="success" />-->
        <br />
      </div>
    </div>
  </apex:form>
  <script>
    function scrollTop() {
      document.querySelector("form").scrollIntoView();
    }
  </script>
